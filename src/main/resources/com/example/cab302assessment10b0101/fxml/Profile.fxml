<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cab302assessment10b0101.controllers.ProfileController">
    <Label layoutX="49.0" layoutY="55.0" prefHeight="23.0" prefWidth="118.0" text="Username">
        <font>
            <Font name="System Bold" size="24.0"/>
        </font>
    </Label>
    <TextField fx:id="usernameTextField" editable="false" layoutX="49.0" layoutY="90.0" prefHeight="35.0"
               prefWidth="195.0"/>
    <Label layoutX="49.0" layoutY="143.0" prefHeight="23.0" prefWidth="118.0" text="Password">
        <font>
            <Font name="System Bold" size="24.0"/>
        </font>
    </Label>
    <TextField fx:id="passwordTextField" editable="false" layoutX="49.0" layoutY="178.0" prefHeight="35.0"
               prefWidth="195.0"/>
    <Button fx:id="changeUsernameButton"
            layoutX="248.0" layoutY="90.0" mnemonicParsing="false" prefHeight="35.0"
            prefWidth="82.0" text="Change" onAction="#handleUpdateUsername"/>
    <Button fx:id="changePasswordButton"
            layoutX="248.0" layoutY="178.0" mnemonicParsing="false" prefHeight="35.0"
            prefWidth="82.0" text="Change" onAction="#handleUpdatePassword"/>
</AnchorPane>
